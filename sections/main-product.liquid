<section id="MainProduct-{{ section.id }}" class="section-{{ section.id }}-padding section-{{ section.id }}-margin" data-section="{{ section.id }}">
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {%- if product.quantity_price_breaks_configured? -%}
    {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
  {%- endif -%}

  {%- style -%}
    .thumbnail-slider{
      border: 1px solid {{ settings.border_color }};
    }
    {% if section.settings.gallery_layout == 'thumbnail_slider_verticle'  or section.settings.gallery_layout == 'thumbnail_right_slider_verticle' %}
      .swiper-container{
        max-height: 750px;
      }
      .thumbnail-slider{
        max-width: 22%;
        width: 100%;
        float: left;
        display: block;
        padding: 16px 15px;
      }
      .main_product_img{
        max-width: 78%;
        width: 100%;
        float: left;
        padding: 0px 15px;
      }
      .product__media-wrapper media-gallery{
        display: inline-flex;
        width: 100%;
        flex-direction: row-reverse;
      }
      .product__media-wrapper .swiper-vertical .swiper-button-prev{
        top: 0;
        left: 50% !important;
        rotate: 90deg;
        translate: -50%;
        transform: translateY(0) !important;
      }
      .product__media-wrapper .swiper-vertical .swiper-button-next{
        bottom: 0;
        top: auto;
        left: 50%;
        rotate: 90deg;
        translate: -50%;
        transform: translateY(0) !important;
      }
       @media screen and (min-width: 992px){
        .product__media-item:not(.is-active), .product__media-item:not(.is-active) {
          display: none;
        }
      }
      @media screen and (max-width: 1366px){
        .swiper-container {
          max-height: 480px;
        }
      }
      @media screen and (max-width: 1199px){
        .thumbnail-slider{
          max-width: 25%;
        }
        .main_product_img{
          max-width: 75%;
        }
      }
      @media screen and (max-width: 991px){
        .product--thumbnail_slider_verticle .product__media-item, .product--thumbnail_right_slider_verticle .product__media-item{
           width: 100%;
         }
        .thumbnail-slider{
          display: none;
        }
        .main_product_img{
          max-width: 100%;
        }
      }
      {% if section.settings.gallery_layout == 'thumbnail_right_slider_verticle' %}
        .product__media-wrapper media-gallery{
          flex-direction: revert;
        }
      {% endif %}
    {% endif %}
    .product--thumbnail_slider .swiper-container{
      height: auto;
    }
    .product--thumbnail_slider .thumbnail-list, .product--thumbnail_slider_verticle .thumbnail-list, .product--thumbnail_right_slider_verticle .thumbnail-list, .product--horizontal_thumbnail .thumbnail-list {
      display: flex;
      flex-wrap: unset;
      grid-gap: 0;
    }
    .product--thumbnail_slider .swiper-slide{
      width: 21.50%;
    } 
    @media screen and (min-width: 750px){
      .product--product_sticky .thumbnail-slider, .product--stacked .thumbnail-slider, .product--columns .thumbnail-slider{
        display: none;
      }
      .slider-button{
        display: none;
      }
    }
    .countdown-timer .main-c-motel{
      background-color: {{ settings.input_bg_color }};
      color:{{ settings.input_text_color }};
    }
    .sticky_cart{
      border-top: 1px solid {{ section.settings.border_color }};
      background-color: {{ section.settings.sticky_bg }};
    }
    .sticky_cart .sticky-page .h3{
      color: {{ settings.card_product_title_color }};
    }
    .sticky_cart .price-item.price-item--regular{
      color: {{ settings.card_product_sale_price_color }};
    }
    .sticky_cart .price-item.price-item--sale{
       color: {{ settings.card_product_regular_price_color }};
    }
    .sticky_cart .quantity:after{
      border: 1px solid {{ settings.q_button_border }};
    }
    .sticky_cart .quantity__input{
      color: {{ settings.q_button_color }};
    }
    .sticky_cart .quantity__button{
      background-color: {{ settings.q_button_bg }} !important;
      color: {{ settings.q_button_color }};
    }
    .sticky_cart .quantity__button:hover{
      background-color: {{ settings.q_button_hr_bg }} !important;
      color: {{ settings.q_button_hr_color }};
    }
    .sticky_cart select, .sticky_cart .quick-add__submit{
       background-color: {{ settings.button_bg }};
      color: {{ settings.button_text }};
    }
    .sticky_cart select:hover, .sticky_cart .quick-add__submit:hover{
       background-color: {{ settings.button_hr_bg }};
      color: {{ settings.button_hr_text }};
    }
    .sticky_cart .sticky_cart_close{
      color: {{ settings.page_title_text_color }};
    }
    .sticky_cart .sticky_cart_close:hover{
      color: {{ settings.theme_text_hover_color }}
    }
    .complementary-products .accordion summary {
      background:{{ settings.input_bg_color }};
      color: {{ settings.input_text_color }};
    }
    .product__info-wrapper .accordion details {
      border: 1px solid {{ settings.border_color }};
      border-radius: var(--buttons-radius-outset);
    }
    {% for block in section.blocks %}
      {% if block.type == 'text' %}
        .product__info-wrapper .product__text{
          color: {{ block.settings.text_color }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'title' %}
        .product__info-wrapper .product__title h1,
        .product__info-wrapper .product__title .h1{
          color: {{ block.settings.product_title_color }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'price' %}
        .product__info-wrapper .price-item.price-item--regular{
          color: {{ block.settings.regular_price }};
        }
        .product__info-wrapper .price-item.price-item--sale{
          color: {{ block.settings.sale_price }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'sku' %}
        .product__info-wrapper .product__sku{
          color: {{ block.settings.sku_heading }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'comparewishlist' %}
      .product__info-wrapper .product-hover button{
        background-color: {{ settings.card_button_bg }};
        color: {{ settings.card_button_text }};
      }
      .product__info-wrapper .quick-add__submit:hover,
      .product__info-wrapper .product-hover button:hover{
        background-color: {{ settings.card_button_hr_bg }};
        color: {{ settings.card_button_hr_text }};
      }
      .product__info-wrapper .product-hover .active .icon.icon-wishlist,
      .product__info-wrapper .product-hover .active .icon.icon-compare{
        fill: {{ settings.card_button_text }};
        color: {{ settings.card_button_text }};
      }
      .product__info-wrapper .product-hover button:hover.active .icon.icon-wishlist,
      .product__info-wrapper .product-hover button:hover.active .icon.icon-compare{
        fill: {{ settings.card_button_hr_text }};
        color: {{ settings.card_button_hr_text }};
      }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'variant_picker' %}
        .varians_heading{
            background-color: {{ block.settings.variant_bg }};
         }
        .product__info-wrapper fieldset.product-form__input .form__label, .varians_heading{
          color: {{ block.settings.var_heading }};
        }
        .product__info-wrapper .product-form__input input[type=radio]:checked+label{
          border: 1px solid {{ block.settings.active_variant_color }};
          background-color: {{ block.settings.active_variant_bg }};
          color: {{ block.settings.active_variant_color }};
        }
        .product__info-wrapper .product-form__input input[type=radio]+label{
          border: 1px solid {{ block.settings.variant_color }};
          background-color: {{ block.settings.variant_bg }};
          color: {{ block.settings.variant_color }};
        }
       {% endif %}
    {% endfor %} 
    
    {% for block in section.blocks %}
      {% if block.type == 'quantity_selector' %}
        .product__info-wrapper .product-form__quantity .form__label{
          color: {{ block.settings.quantity_heading }};
        }
      {% endif %}
    {% endfor %} 
    .product__info-wrapper .quantity:after{
      border: 1px solid {{ settings.q_button_border }};
    }
    .product__info-wrapper .quantity__input{
      color: {{ settings.q_button_color }};
    }
    .product__info-wrapper .product-form__quantity .quantity__button{
      background-color: {{ settings.q_button_bg }} !important;
      color: {{ settings.q_button_color }};
    }
    .product__info-wrapper .product-form__quantity .quantity__button:hover{
      background-color: {{ settings.q_button_hr_bg }} !important;
      color: {{ settings.q_button_hr_color }};
    }
    {% for block in section.blocks %}
      {% if block.type == 'buy_buttons' %}
        .product__info-wrapper .product-form__submit{
          background-color: {{ block.settings.add_btn_bg }};
          color: {{ block.settings.add_btn_color }};
        }
        .product__info-wrapper .product-form__submit:hover{
          background-color: {{ block.settings.add_btn_hr_bg }};
          color: {{ block.settings.add_btn_hr_color }};
        }
        .product__info-wrapper .shopify-payment-button__button{
          background-color: {{ block.settings.add_btn_hr_bg }};
          color: {{ block.settings.add_btn_hr_color }};
        }
        .product__info-wrapper .shopify-payment-button__button:hover{
          background-color: {{ block.settings.add_btn_bg }};
          color: {{ block.settings.add_btn_color }};
        }
        .product__info-wrapper .product_category_feature .form__label{
          color: {{ block.settings.collection_label_color }};
        }
       .product__info-wrapper .product_category_feature .main_collection{
          color: {{ block.settings.collection_color }};
          border-bottom: 1px solid {{ block.settings.collection_color }};
        }
        .product__info-wrapper .product_category_feature .main_collection:hover{
          color: {{ block.settings.collection_hr_color }};
          border-bottom: 1px solid {{ block.settings.collection_hr_color }};
        }
      {% endif %}
    {% endfor %} 
    {% for block in section.blocks %}
      {% if block.type == 'inventory' %}
        .product__info-wrapper .product__inventory{
          color: {{ block.settings.inventory_text }};
        }
      {% endif %}
    {% endfor %} 
    {% for block in section.blocks %}
      {% if block.type == 'popup' %}
        .product-popup-modal__opener .product-popup-modal__button{
          color: {{ block.settings.sizechart_color }};
        }
      {% endif %}
    {% endfor %} 
    {% for block in section.blocks %}
      {% if block.type == 'icon-with-text' %}
        .icon-with-text__item svg, .icon-with-text__item span{
          fill: {{ block.settings.icon_block_text }};
          color: {{ block.settings.icon_block_text }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'collapsible_tab' %}
        .product__accordion .icon-caret, .product__accordion .icon-accordion, .product__accordion .accordion__title{
          fill: {{ block.settings.acco_title }} !important;
          color: {{ block.settings.acco_title }};
        }
        .product__accordion .accordion__content{
          color: {{ block.settings.acco_content }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'description' %}
        .product__description{
          color: {{ block.settings.description_color }};
        }
      {% endif %}
    {% endfor %}
    {% for block in section.blocks %}
      {% if block.type == 'view' %}
        .product__info-wrapper .visitors_user{
          background-color: {{ block.settings.visitor_bg }};
          color:{{ block.settings.visitor_text }}; 
          fill:{{ block.settings.visitor_text }}; 
        }
      {% endif %}
      {% if block.type == 'product_sales' %}
        .social-proof-sales{
          background-color: {{ block.settings.sale_bg }};
          color:{{ block.settings.sale_text }}; 
          fill:{{ block.settings.sale_text }}; 
        }
      {% endif %}
    {% endfor %}
    .complementary-products__container .product-card-wrapper .card__heading a{
      color: {{ settings.card_product_title_color }};
    }
    .complementary-products__container .product-card-wrapper .card__heading a:hover{
      color: {{ settings.card_product_title_hr_color }};
    }
    .complementary-products__container .product-card-wrapper .price--on-sale .price-item--regular {
      color: {{ settings.card_product_sale_price_color }};
    }
    .complementary-products__container .product-card-wrapper .price--on-sale .price-item--last {
      color: {{ settings.card_product_regular_price_color }};
    }
    .badge{
      background-color: {{ settings.badge_bg_color }};
      color: {{ settings.badge_text_color }};
    }
    .product__info-wrapper .product__tax,
    .pickup-availability-preview .pickup-availability-button,
    .pickup-availability-header .pickup-availability-drawer-title,
    .pickup-availability-list__item .h4,
    .pickup-availability-preview .pickup-availability-button:hover,
    .recipient-checkbox>svg,
    .recipient-checkbox span,
    .recipient-form-field-label span{
      color: {{ section.settings.text_heading_color }};
    }
    .pickup-availability-preview .caption-large,
    .pickup-availability-preview .caption-large span,
    .pickup-availability-preview .caption,
    .pickup-availability-variant,
    .pickup-availability-preview svg path,
    .pickup-availability-preview,
    .pickup-availability-address{
      color: {{ section.settings.text_description_color }};
      fill: {{ section.settings.text_description_color }};
    }
    pickup-availability-drawer[open]{
      background-color: {{ section.settings.drawer_bg }};
    }
    .pickup-availability-drawer-button,
    .share-button__button,
    .share-button__fallback button,
    .product-popup-modal__toggle{
      background-color: {{ settings.button_bg }};
      color: {{ settings.button_text }};
    }
    .pickup-availability-drawer-button:hover,
    .share-button__button:hover,
    .share-button__fallback button:hover,
    .product-popup-modal__toggle:hover{
      background-color: {{ settings.button_hr_bg }};
      color: {{ settings.button_hr_text }};
    }
    .product__media-wrapper .slider-mobile-gutter .slider-button.slider-button--prev,
    .product__media-wrapper .slider-mobile-gutter .slider-button.slider-button--next,
    .product__media-wrapper .swiper-button-next,
    .product__media-wrapper .swiper-button-prev{
      background-color: {{ settings.button_bg }};
      color: {{ settings.button_text }};
    }
    .product__media-wrapper .slider-mobile-gutter .slider-button.slider-button--prev:hover,
    .product__media-wrapper .slider-mobile-gutter .slider-button.slider-button--next:hover,
    .product__media-wrapper .swiper-button-next:hover,
    .product__media-wrapper .swiper-button-prev:hover{
      background-color: {{ settings.button_hr_bg }};
      color: {{ settings.button_hr_text }};
    }
    .product__info-wrapper variant-radios,
    .icon-with-text--horizontal .icon-with-text__item{
      border: 1px solid {{ section.settings.border_color }};
    }
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top }}px;
      padding-bottom: {{ section.settings.padding-bottom }}px;
    }
    @media screen and (max-width: 1429px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding-top-1429 }}px;
        padding-bottom: {{ section.settings.padding-bottom-1429 }}px;
      }
    }
    @media screen and (max-width: 1199px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding-top-1199 }}px;
        padding-bottom: {{ section.settings.padding-bottom-1199 }}px;
      }
    }
    @media screen and (max-width: 991px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding-top-991 }}px;
        padding-bottom: {{ section.settings.padding-bottom-991 }}px;
      }
    }
    @media screen and (max-width: 767px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding-top-767 }}px;
        padding-bottom: {{ section.settings.padding-bottom-767 }}px;
      }
    }
    @media screen and (max-width: 575px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding-top-575 }}px;
        padding-bottom: {{ section.settings.padding-bottom-575 }}px; 
      }
    }
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
    <script src="{{ 'price-per-item.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div class="page-width">
    <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media{% endif %}">
    <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
      {% render 'product-media-gallery', variant_images: variant_images %}
    </div>
    <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
      <product-info
        id="ProductInfo-{{ section.id }}"
        data-section="{{ section.id }}"
        data-url="{{ product.url }}"
        class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
      >
        {%- assign product_form_id = 'product-form-' | append: section.id -%}

        {%- for block in section.blocks -%}
          {%- case block.type -%}
            {%- when '@app' -%}
              {% render block %}
            {%- when 'text' -%}
              <p
                class="product__text inline-richtext"
                {{ block.shopify_attributes }}
              >
                {{- block.settings.text -}}
              </p>
            {%- when 'title' -%}
              <div class="product__title" {{ block.shopify_attributes }}>
                <h1 class="h2">{{ product.title | escape }}</h1>
                <a href="{{ product.url }}" class="product__title">
                  <div class="h1">
                    {{ product.title | escape }}
                  </div>
                </a>
              </div>
              {% assign product_metafields = product.metafields %}
              {% assign countdown_date = product_metafields.card_product.countdown_date %}
            
                {% if countdown_date %}
                  {% assign countdown_time = countdown_date | date: '%s' %}
                  {% assign current_time = 'now' | date: '%s' %}
                  {% assign time_diff = countdown_time | minus: current_time %}
                  {% assign days_left = time_diff | divided_by: 86400 %}
                  {% assign hours_left = time_diff | divided_by: 3600 | modulo: 24 %}
                  {% assign minutes_left = time_diff | divided_by: 60 | modulo: 60 %}
                  {% assign seconds_left = time_diff | modulo: 60 %}
                
                  <div class="countdown-timer countdown__wrapper clockdiv" id="ui-countdown" data-date="{{ countdown_date  | date: '%B %d, %Y %H:%M:%S' }}">
  
                    <div class="main-c-motel">
                      <div class="countdown-item days countdown__value" id="ui-countdown-days">{{ days_left }}</div>
                      <div class="bigtext_countdown">{{ 'products.product.quantity.day' | t }}</div>
                    </div>
                     <div class="main-c-motel">
                    <div class="countdown-item hours countdown__value" id="ui-countdown-hours">{{ hours_left }}</div>
                    <div class="bigtext_countdown">{{ 'products.product.quantity.hours' | t }}</div>
                   </div>
                    <div class="main-c-motel">
                      <div class="countdown-item minutes countdown__value" id="ui-countdown-minutes">{{ minutes_left }}</div>
                      <div class="bigtext_countdown">{{ 'products.product.quantity.min' | t }}</div>
                    </div>
                    <div class="main-c-motel">
                      <div class="countdown-item seconds countdown__value" id="ui-countdown-seconds">{{ seconds_left }}</div>
                      <div class="bigtext_countdown">{{ 'products.product.quantity.sec' | t }}</div>
                    </div>
                   
                  </div>
              {% endif %}
            {%- when 'view' -%}
              <div class="visitors_user">
                  <div class="eye_user">{% render 'icon-eye' %}</div>
                  <div id='number' style="display: block;"></div>
                  <span>{{ block.settings.count_text }}</span>
              </div>
              <script>
                document.addEventListener('DOMContentLoaded', function() {
                  // Function to generate a random integer between min and max
                  function randomInt(min, max) {
                    return Math.floor(Math.random() * (max - min)  + min);
                  }
              
                  // Function to update the numberDiv with a random number
                  function updateNumberDiv(min, max) {
                    var numberDiv = document.getElementById('number');
                    numberDiv.textContent = randomInt(min, max);
                  }
              
                  // Function to call updateNumberDiv at regular intervals
                  function getNumberBetweenEvery(min, max, secs) {
                    updateNumberDiv({{ block.settings.count_range_min }}, {{ block.settings.count_range_max }}); // Initial call to display a random number
              
                    setInterval(function() {
                      updateNumberDiv({{ block.settings.count_range_min }}, {{ block.settings.count_range_max }});
                    }, secs * {{ block.settings.view_duration }}); // Convert secs to milliseconds
                  }
              
                  // Call getNumberBetweenEvery with your desired parameters
                  getNumberBetweenEvery(100, 1000, 1);
                });
              </script>
            {%- when 'price' -%}
              <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                {%- render 'price',
                  product: product,
                  use_variant: true,
                  show_badges: true,
                  price_class: 'price--large'
                -%}
              </div>
              {%- if product.quantity_price_breaks_configured? -%}
                <div class="volume-pricing-note" id="Volume-Note-{{ section.id }}">
                  <span>{{ 'products.product.volume_pricing.note' | t }}</span>
                </div>
              {%- endif -%}
                <div class="product__tax caption rte">
                  {%- if cart.taxes_included -%}
                    {{ 'products.product.include_taxes' | t }}
                  {%- endif -%}
                  {%- if shop.shipping_policy.body != blank -%}
                    {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                  {%- endif -%}
                </div>
              <div {{ block.shopify_attributes }}>
                {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
