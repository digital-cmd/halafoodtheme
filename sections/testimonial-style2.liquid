{{ 'testimonial-style2.css' | asset_url | stylesheet_tag }}
{{ 'component-slider.css' | asset_url | stylesheet_tag }}

{%- style -%}
  .testimonial-main-wrapper2 .swiper-button-next,
  .testimonial-main-wrapper2 .swiper-button-prev{
    background: {{ settings.button_bg }}; 
    color: {{ settings.button_text }};
  }
  .testimonial-main-wrapper2 .swiper-button-next:hover,
  .testimonial-main-wrapper2 .swiper-button-prev:hover{
    background: {{ settings.button_hr_bg }}; 
    color: {{ settings.button_hr_text }};
  }
  .testimonial-main-wrapper2 .swiper-pagination-bullet { 
    background: {{ settings.theme_dots }}; 
  }  
  .testimonial-main-wrapper2 .swiper-pagination-bullet.swiper-pagination-bullet-active { 
    background: {{ settings.theme_dots_active }};
  }
  .testimonial-main-wrapper2 .testimonial-list-subtitle{
    color: {{ section.settings.subheading_color }};
  }
  .testimonial-main-wrapper2 .testimonial-list-title{
    color: {{ section.settings.heading_color }};
  }
 .testimonial-main-wrapper2 .testimonial-list-description{
    color: {{ section.settings.content_color }};
  }
  .testimonial-main-wrapper2 .testimonial__title{
    text-align: {{ section.settings.main_title_alignment }};
  }
  .testimonial-main-wrapper2 .testimonials-list__item--left {
    text-align: left;
  }
  .testimonial-main-wrapper2 .testimonials-list__item--center {
    text-align: center;
  }
  .testimonial-main-wrapper2 .testimonials-list__item--right {
    text-align: right;
  }
  .testimonial-main-wrapper2 .testimonials-list__item--left .testimonial-adition,
  .testimonial-main-wrapper2 .testimonials-list__item--left .testimonials-card .card:not(.ratio) {
    justify-content: start;
  }
  .testimonial-main-wrapper2 .testimonials-list__item--center .testimonial-adition,
  .testimonial-main-wrapper2 .testimonials-list__item--center .testimonials-card .card:not(.ratio) {
    justify-content: center;
  }
  .testimonial-main-wrapper2 .testimonials-list__item--right .testimonial-adition,
  .testimonial-main-wrapper2 .testimonials-list__item--right .testimonials-card .card:not(.ratio) {
    justify-content: end;
  }
  .testimonial-main-wrapper2 .review_description p{
    color: {{ section.settings.review_text_color }};
  }
  .testimonial-main-wrapper2 .testimonial-ratings svg{
    fill: {{ section.settings.rating_star_color }};
  }
  .testimonial-main-wrapper2 .author-name-tag .h3, .testimonial-main-wrapper2 .testi_dash{
    color: {{ section.settings.author_name_color }};
  }
  .testimonial-main-wrapper2 .author-designation-tag .h3{
    color: {{ section.settings.author_designation_color }};
  }
  .testimonial-main-wrapper2 svg{
    fill: {{ section.settings.quote_color }};
  }
  .testimonial-main-wrapper2 .product_to_review {
    border-top: 1px solid {{ section.settings.review_text_color | color_modify: 'alpha', 0.10 }};
  }
  .testimonial-main-wrapper2 .testimonial_main_tap .heading-img.center{
    margin: 0 auto;
  }
  .testimonial-main-wrapper2 .testimonial_main_tap .heading-img.right{
    margin: 0 0 0 auto;
  }
  .testimonial-main-wrapper2 .testimonial-ratings{
    justify-content: {{ section.settings.column_alignment}};
  }
  .testimonial-main-wrapper2 .card:not(.ratio) {
    justify-content: {{ section.settings.column_alignment}};
  }
  .section-{{ section.id }}-padding {
    padding-top: {{ section.settings.padding-top }}px;
    padding-bottom: {{ section.settings.padding-bottom }}px;
    margin-top: {{ section.settings.margin-top }}px;
    margin-bottom: {{ section.settings.margin-bottom }}px;
    background-color: {{ section.settings.section_bg }};
  }
  @media screen and (max-width: 1429px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top-1429 }}px;
      padding-bottom: {{ section.settings.padding-bottom-1429 }}px;
      margin-top: {{ section.settings.margin-top-1429 }}px;
      margin-bottom: {{ section.settings.margin-bottom-1429 }}px;
    }
  }
  @media screen and (max-width: 1199px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top-1199 }}px;
      padding-bottom: {{ section.settings.padding-bottom-1199 }}px;
      margin-top: {{ section.settings.margin-top-1199 }}px;
      margin-bottom: {{ section.settings.margin-bottom-1199 }}px;
    }
  }
  @media screen and (max-width: 991px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top-991 }}px;
      padding-bottom: {{ section.settings.padding-bottom-991 }}px;
      margin-top: {{ section.settings.margin-top-991 }}px;
      margin-bottom: {{ section.settings.margin-bottom-991 }}px;
    }
  }
  @media screen and (max-width: 767px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top-767 }}px;
      padding-bottom: {{ section.settings.padding-bottom-767 }}px;
      margin-top: {{ section.settings.margin-top-767 }}px;
      margin-bottom: {{ section.settings.margin-bottom-767 }}px;  
    }
  }
  @media screen and (max-width: 575px) {
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding-top-575 }}px;
      padding-bottom: {{ section.settings.padding-bottom-575 }}px;
      margin-top: {{ section.settings.margin-top-575 }}px;
      margin-bottom: {{ section.settings.margin-bottom-575 }}px;  
    }
  }
{%- endstyle -%}

{%- liquid
  assign show_desktop_slider = false
  if section.settings.enable_desktop_slider and section.blocks.size >= section.settings.columns_desktop
    assign show_desktop_slider = true
  endif
-%}

<div class="{{ section.settings.section_width }}">
  <div class="testimonial-main-wrapper2 {% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %} section-{{ section.id }}-padding ">
    <div class="collection testimonial_main_tap isolate {% if section.settings.section_width == 'padding_width' or section.settings.section_width == 'full_width' and section.settings.testimonial_style == 'classic' %}page-width{% endif %}"
      {% if settings.animations_reveal_on_scroll %}
        data-cascade
      {% endif %}
    >
           <div class="testimonial-main-inner2 {{ section.settings.testimonial_style }} {{ section.settings.position }}">
               <div class="user-img">
                <div class="image-main-wrap">
                <div class="image-main-testimonial">
                    <div class="first-img">
                      <div class="testimonial-hover-img">
                        {% if section.settings.author_img1 != blank %}
                          <div class="media media--transparent ratio" style="--ratio-percent: 140.7%;">
                            {{ section.settings.author_img1 | image_url: width: 1920 | image_tag : loading: 'lazy', class: 'w-100' }}
                          </div>
                          {% else %}
                          <div class="media media--transparent ratio" style="--ratio-percent: 140.7%;">
                            {{ 'collection-6' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                          </div>
                          {% endif %}
                          </div>
                      </div>
                  </div>
                  <div class="third-img">
                    <div class="testimonial-hover-img">
                      {% if section.settings.author_img3 != blank %}
                        <div class="media media--transparent ratio" style="--ratio-percent: 124%;">
                          {{ section.settings.author_img3 | image_url: width: 1920 | image_tag : loading: 'lazy', class: 'w-100' }}
                        </div>
                        {% else %}
                        <div class="media media--transparent ratio" style="--ratio-percent: 124%;">
                          {{ 'collection-6' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                        </div>
                        {% endif %}
                      </div>
                    </div>
                   <div class="second-img">
                      <div>
                      {% if section.settings.author_img2 != blank %}
                        <div class="media media--transparent ratio" style="--ratio-percent: 46%;">
                          {{ section.settings.author_img2 | image_url: width: 1920 | image_tag : loading: 'lazy', class: 'w-100' }}
                        </div>
                        {% else %}
                        <div class="media media--transparent ratio" style="--ratio-percent: 46%;">
                          {{ 'collection-6' | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                        </div>
                        {% endif %}
                      </div>
                    </div>
                  </div> 
            </div>
          <div class="testimonial-wrap2">
              <div class="testimonial-sec2">
                {% if section.settings.heading-img != blank or section.settings.t_subtitle != blank or section.settings.t_title != blank or section.settings.t_description != blank %}
                  <div class="testimonial__title title_bottom_margin title-wrapper">
                    {% if section.settings.heading-img != blank %}
                      <div class="heading-img w-100 {{ section.settings.main_title_alignment }}">
                        <img src="{{ section.settings.heading-img | img_url: 'master' }}" alt="slider-img" class="w-100" loading="eager">
                      </div>
                    {% endif %}
                    {% if section.settings.t_subtitle != blank %}
                      <div class="testimonial-list-subtitle theme_main_subheading">{{ section.settings.t_subtitle }}</div>
                    {% endif %}
                    {% if section.settings.t_title != blank %}
                      <div class="testimonial-list-title theme_main_heading h2">{{ section.settings.t_title }}</div>
                    {% endif %}
                    {% if section.settings.t_description != blank %}
                      <div class="testimonial-list-description theme_main_description">{{ section.settings.t_description }}</div>
                    {% endif %}
                  </div>
                {% endif %}
                    <slider-component class="swiper-container testimonial-slider2"
                    data-desktop="{{ section.settings.columns_desktop }}"
                    data-tablet="{{ section.settings.columns_tablet }}" data-mobile="{{ section.settings.columns_mobile }}" data-autoplay="{{ section.settings.autoplay_enable }}"
                    data-loop="{{ section.settings.loop }}" data-speed="{{ section.settings.speed }}">
                    <ul
                      class="testimonials-list contains-content-container swiper-wrapper"
                      id="Slider-{{ section.id }}"
                      role="list"
                    >
                      {%- liquid
                        assign highest_ratio = 0
                        for block in section.blocks
                          if block.settings.image.aspect_ratio > highest_ratio
                            assign highest_ratio = block.settings.image.aspect_ratio
                          endif
                        endfor
                      -%}
                      {%- for block in section.blocks -%}
                        <li
                          id="Slide-{{ section.id }}-{{ forloop.index }}"
                          class="testimonials-list__item testimonials-list__item--{{ section.settings.column_alignment }} swiper-slide"
                          {{ block.shopify_attributes }}
                          {% if settings.animations_reveal_on_scroll %}
                            data-cascade
                            style="--animation-order: {{ forloop.index }};"
                          {% endif %}
                        >
                          <div class="testi-inner content-container column-{{ block.id }}">
                            
                              <svg xmlns="http://www.w3.org/2000/svg" version="1.0" width="29.000000pt" height="26.000000pt"
                                viewBox="0 0 29.000000 26.000000" preserveAspectRatio="xMidYMid meet" class="testimonial-quates">
                                <g transform="translate(0.000000,26.000000) scale(0.100000,-0.100000)" stroke="none">
                                  <path
                                    d="M81 205 c-48 -53 -71 -100 -71 -148 0 -38 15 -57 45 -57 49 0 75 77 35 102 -28 18 -25 37 11 92 17 26 29 49 27 52 -3 2 -24 -16 -47 -41z" />
                                  <path
                                    d="M235 212 c-45 -45 -74 -104 -75 -152 0 -41 14 -60 45 -60 50 0 74 75 34 104 -27 19 -20 53 21 100 42 47 22 54 -25 8z" />
                                </g>
                              </svg>
                            
                            <div class="testimonials-card__info">
                              {%- if block.settings.text != blank -%}
                                <div class="review_description">{{ block.settings.text }}</div>
                              {%- endif -%}
                              {%- if block.settings.show_rating -%}
                                <div class="testimonial-ratings">
                                  {% if block.settings.first_star %}
                                  {% render 'icon-star' %}
                                  {% endif %}
                                  {% if block.settings.second_star %}
                                  {% render 'icon-star' %}
                                  {% endif %}
                                  {% if block.settings.third_star %}
                                  {% render 'icon-star' %}
                                  {% endif %}
                                  {% if block.settings.fourth_star %}
                                  {% render 'icon-star' %}
                                  {% endif %}
                                  {% if block.settings.half_star == false %}
                                  {% if block.settings.fifth_star %}
                                  {% render 'icon-star' %}
                                  {% endif %}
                                  {% else %}
                                  {% if block.settings.half_star %}
                                  {% render 'icon-half-star' %}
                                  {% endif %}
                                  {% endif %}
                                </div>
                              {%- endif -%}
                              <div class="testimonial-adition">
                                {%- if block.settings.author_name != blank -%}
                                <div class="author-name-tag">
                                  <div class="h3">{{ block.settings.author_name }}</div>
                                </div>
                                {% endif %}
                                <div class="testi_dash"> - </div>
                                {%- if block.settings.author_designation != blank -%}
                                <div class="author-designation-tag">
                                  <div class="h3">{{ block.settings.author_designation }}</div>
                                </div>
                                {% endif %}
                              </div>
                              {% if block.settings.review_product %}
                                <div class="product_to_review">
                                  {% if block.settings.product != blank %}
                                  {%- assign product = all_products[block.settings.product] -%}
                                  {% render 'card-product',
                                  card_product: product,
                                  show_secondary_image: section.settings.show_secondary_image,
                                  show_vendor: section.settings.show_vendor,
                                  show_rating: section.settings.show_rating,
                                  section_id: section.id
                                  %}
                                  {% else %}
                                  <div class="card">
                                    <a class="card__inner">
                                      {% capture current %}{% cycle 1, 2, 3, 4, 5, 6 %}{% endcapture %}
                                      {{ 'product-' | append: current | placeholder_svg_tag: 'placeholder-svg placeholder' }}
                                    </a>
                                    <div class="product-collection__content">
                                      <div class="product-collection__title h4">
                                        <a class="cd">{{ 'onboarding.product_title' | t }}</a>
                                      </div>
                                      <span class="price price--sale">
                                        <span class="current"><span class="money">$.59.00</span></span>
                                      </span>
                                    </div>
                                  </div>
                                  {% endif %}
                                </div>
                              {% endif %}
                            </div>
                          </div>
                        </li>
                      {%- endfor -%}
                    </ul>
                    {% if section.settings.arrows%}
                      <div class="swiper-buttons">
                        <div class="swiper-button-next testimonial2-swiper-button-next">{% render 'icon-caret' %}</div>
                        <div class="swiper-button-prev testimonial2-swiper-button-prev">{% render 'icon-caret' %}</div>
                      </div>
                    {% endif %}
                    {% if section.settings.dots-%}
                      <div class="swiper-pagination testimonial2-swiper-pagination" id="swiper-pagination-{{section.id}}" style="position: inherit;"></div>
                    {% endif %}
                    
                    </slider-component>
            </div>
        </div>
     </div>
  </div>
</div>
</div>
                      
{% schema %}
{
  "name": "Testimonial style2",
  "class": "section",
  "tag": "section",
  "disabled_on": {
    "groups": ["header", "footer"]
  },
  "settings": [
    {
      "type": "select",
      "id": "section_width",
      "options": [
        {
          "value": "full_width",
          "label": "Full width"
        },
        {
          "value": "padding_width",
          "label": "With padding"
        }
      ],
      "label": "Section width",
      "default": "full_width"
   },
   {
      "type": "select",
      "id": "testimonial_style",
      "options": [
        {
          "value": "classic",
          "label": "Classic style"
        },
        {
          "value": "modern",
          "label": "Modern style"
        }
      ],
      "default": "classic",
      "label": "Testimonial style"
    },
    {
      "type": "select",
      "id": "position",
      "options": [
        {
          "value": "left",
          "label": "Left image"
        },
        {
          "value": "right",
          "label": "Right image"
        }
      ],
      "default": "left",
      "label": "Image style"
    },
   {
      "type" : "image_picker",
      "id" : "heading-img",
      "label" : "Heading image"
    },
    {
      "type": "text",
      "id": "t_subtitle",
      "label": "t:sections.featured-collection.settings.subheading.label",
      "default": "Client review"
    },
    {
      "type": "textarea",
      "id": "t_title",
      "label": "t:sections.multicolumn.settings.title.label",
      "default": "Testimonial"
    },
    {
      "type": "textarea",
      "id": "t_description",
      "label": "t:sections.featured-collection.settings.description.label",
      "default": "Write reviews of your happy clients."
    },
    {
      "type": "select",
      "id": "main_title_alignment",
      "options": [
        {
          "value": "left",
          "label": "t:sections.social_media_collage.settings.left"
        },
        {
          "value": "center",
          "label": "t:sections.social_media_collage.settings.center"
        },
        {
          "value": "right",
          "label": "t:sections.social_media_collage.settings.right"
        }
      ],
      "default": "center",
      "label": "t:sections.social_media_collage.settings.main_title_alignment"
    },
    {
      "type": "range",
      "id": "columns_desktop",
      "min": 1,
      "max": 3,
      "step": 1,
      "default": 1,
      "label": "t:sections.featured-collection.settings.columns_desktop.label"
    },
    {
      "type": "range",
      "id": "columns_tablet",
      "min": 1,
      "max": 3,
      "step": 1,
      "default": 1,
      "label": "Number of columns on tablet"
    },
    {
      "type": "range",
      "id": "columns_mobile",
      "min": 1,
      "max": 3,
      "step": 1,
      "default": 1,
      "label": "Number of columns on mobile"
    },
    {
      "type": "checkbox",
      "id": "autoplay_enable",
      "label": "Autoplay enable",
      "default": true
    },
    {
      "type": "checkbox",
      "id": "arrows",
      "label": "Enable navigation arrows",
      "default": false
    },
    {
      "type": "checkbox",
      "id": "dots",
      "label": "Enable pagination dots",
      "default": false
    },
    {
      "type": "text",
      "id": "speed",
      "label": "Slide speed",
      "default": "500"
    },
    {
      "type": "checkbox",
      "id": "loop",
      "label": "Enable loop",
      "default": false
    },
    {
      "type": "select",
      "id": "column_alignment",
      "options": [
        {
          "value": "left",
          "label": "t:sections.testimonials.settings.column_alignment.options__1.label"
        },
        {
          "value": "center",
          "label": "t:sections.testimonials.settings.column_alignment.options__2.label"
        },
        {
          "value": "right",
          "label": "t:sections.testimonials.settings.column_alignment.options__3.label"
        }
      ],
      "default": "left",
      "label": "t:sections.social_media_collage.settings.content_alignment"
    },
    {
      "type": "header",
      "content": "Image setting"
    },
    {
      "type": "image_picker",
      "id": "author_img1",
      "label": "Left image",
      "info": "When use 'modern style' use this image"
    },
    {
      "type": "image_picker",
      "id": "author_img2",
      "label": "Top right image"
    },
    {
      "type": "image_picker",
      "id": "author_img3",
      "label": "Right image"
    },
    {
      "type": "header",
      "content": "t:sections.social_media_collage.settings.color_setting"
    },
    {
      "type": "color",
      "id": "section_bg",
      "label": "t:sections.social_media_collage.settings.section_background",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "subheading_color",
      "label": "t:sections.social_media_collage.settings.subheading_color",
      "default": "#131313"
    },
    {
      "type": "color",
      "id": "heading_color",
      "label": "t:sections.social_media_collage.settings.heading_color",
      "default": "#131313"
    },
    {
      "type": "color",
      "id": "content_color",
      "label": "t:sections.social_media_collage.settings.description_color",
      "default": "#666666"
    },
    {
      "type": "color",
      "id": "quote_color",
      "label": "t:sections.testimonials.settings.quotes_color",
      "default": "#ffffff"
    },
    {
      "type": "color",
      "id": "review_text_color",
      "label": "t:sections.testimonials.settings.review_text_color",
      "default": "#131313"
    },
    {
      "type": "color",
      "id": "rating_star_color",
      "label": "t:sections.testimonials.settings.review_star_color",
      "default": "#131313"
    },
    {
      "type": "color",
      "id": "author_name_color",
      "label": "t:sections.testimonials.settings.author_name_color",
      "default": "#131313"
    },
    {
      "type": "color",
      "id": "author_designation_color",
      "label": "t:sections.testimonials.settings.author_designation_color",
      "default": "#131313"
    },
    {
      "type": "header",
      "content": "t:sections.all.padding.section_padding_heading"
    },
    {
      "type": "number",
      "id": "padding-top",
      "label": "t:sections.all.padding.padding_top",
      "default": 120
    },
    {
      "type": "number",
      "id": "padding-bottom",
      "label": "t:sections.all.padding.padding_bottom",
      "default": 120
    },
    {
      "type": "header",
      "content": "Screen wise section padding"
    },
    {
      "type": "header",
      "content": "@media screen 1429"
    },
    {
      "type": "number",
      "id": "padding-top-1429",
      "label": "Padding top",
      "default": 70
    },
    {
      "type": "number",
      "id": "padding-bottom-1429",
      "label": "Padding bottom",
      "default": 70
    },
    {
      "type": "header",
      "content": "@media screen 1199"
    },
    {
      "type": "number",
      "id": "padding-top-1199",
      "label": "Padding top",
      "default": 60
    },
    {
      "type": "number",
      "id": "padding-bottom-1199",
      "label": "Padding bottom",
      "default": 60
    },
    {
      "type": "header",
      "content": "@media screen 991"
    },
    {
      "type": "number",
      "id": "padding-top-991",
      "label": "Padding top",
      "default": 50
    },
    {
      "type": "number",
      "id": "padding-bottom-991",
      "label": "Padding bottom",
      "default": 50
    },
    {
      "type": "header",
      "content": "@media screen 767"
    },
    {
      "type": "number",
      "id": "padding-top-767",
      "label": "Padding top",
      "default": 40
    },
    {
      "type": "number",
      "id": "padding-bottom-767",
      "label": "Padding bottom",
      "default": 40
    },
    {
      "type": "header",
      "content": "@media screen 575"
    },
    {
      "type": "number",
      "id": "padding-top-575",
      "label": "Padding top",
      "default": 30
    },
    {
      "type": "number",
      "id": "padding-bottom-575",
      "label": "Padding bottom",
      "default": 30
    },
    {
      "type": "header",
      "content": "Margin settings"
    },
    {
      "type": "number",
      "id": "margin-top",
      "label": "Margin top",
      "default": 0
